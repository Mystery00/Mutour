<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

    <TextView
        android:id="@+id/item_my_book_text"
        android:layout_width="match_parent"
        android:layout_height="@dimen/my_book_item_height"
        android:layout_marginEnd="@dimen/my_book_item_margin"
        android:layout_marginStart="@dimen/my_book_item_margin"
        android:drawablePadding="@dimen/my_book_item_margin"
        android:drawableStart="@drawable/ic_new_get"
        android:gravity="center_vertical"
        android:textSize="@dimen/my_book_item_text_size"/>

</RelativeLayout>
